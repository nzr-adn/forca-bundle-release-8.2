.landingpage-common-square {
	background-color: #e87722;
	vertical-align: top; 
	text-align:left; 
	margin:2%; 
	padding:10px; 
	height: 116px;
	outline: 1px solid gray;
	position: relative;	
}
.landingpage-common-square:after {
  	content: "";
  	display: block;
}
.landingpage-common-square-label {
    bottom: 0;
    left: 0;
    position: absolute;
    padding-left: 10px;
}
.landingPage .z-grid{
	border : 0px;
}
.landingPage .z-grid-odd>.z-row-inner, .z-grid-odd>.z-cell{
	background : none;
}
.mydiversity .z-select {
	width: 100%
}
.rr-gridview .z-grid-body{
